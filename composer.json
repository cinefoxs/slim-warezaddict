{
    "name": "warezaddict-com/slim-warezaddict",
    "description": "WarezAddict.com website powered by Slim PHP framework",
    "keywords": [
        "warezaddict",
        "movies",
        "torrent",
        "slim"
    ],
    "homepage": "https://github.com/warezaddict-com/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Turbo",
            "email": "none@iHateSpam.biz",
            "homepage": "https://warezaddict.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "funct/funct": "^1.5",
        "illuminate/database": "^5.7",
        "monolog/monolog": "^1.24",
        "nesbot/carbon": "^2.11",
        "php-tmdb/api": "^2.1",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.3",
        "slim/flash": "^0.4.0",
        "slim/slim": "^3.12",
        "slim/twig-view": "^2.4",
        "twig/extensions": "^1.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "zeuxisoo/slim-whoops": "^0.6.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "WarezAddict\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0,
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "opti": "composer -o dumpautoload"
    }
}
